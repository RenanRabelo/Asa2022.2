;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	web.br. root.web.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN  MX 10 mail.web.br.
@	IN	NS	ns1.web.br.
;@	IN	NS	ns2.web.br.
@	IN	A	127.0.0.1
@	IN	AAAA	::1

ns1			A	10.25.1.133
mail		A 	10.25.1.133
www			CNAME	ns1